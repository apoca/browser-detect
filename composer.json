{
  "name": "hisorange/browser-detect",
  "description": "Browser & Mobile detection package for Laravel.",
  "keywords": [
    "laravel",
    "user-agent",
    "browser",
    "mobile",
    "detect",
    "tablet",
    "user agent",
    "mobile",
    "tablet",
    "analyize",
    "hisorange"
  ],
  "homepage": "https://github.com/hisorange/browser-detect",
  "license": "MIT",
  "version": "3.1.4",
  "authors": [
    {
      "name": "Varga Zsolt (hisorange)",
      "email": "hello@hisorange.me"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "laravel/framework": "~5.0|^6.0",
    "ua-parser/uap-php": "~3.8",
    "league/pipeline": "~0.1",
    "mobiledetect/mobiledetectlib": "~2.0",
    "jaybizzle/crawler-detect": "~1.0",
    "piwik/device-detector": "~3.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "~3.0",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "hisorange\\BrowserDetect\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "hisorange\\BrowserDetect\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "hisorange\\BrowserDetect\\ServiceProvider"
      ],
      "aliases": {
        "Browser": "hisorange\\BrowserDetect\\Facade"
      }
    }
  }
}
